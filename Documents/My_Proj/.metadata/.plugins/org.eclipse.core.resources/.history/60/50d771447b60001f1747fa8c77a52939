package basics;
import java.util.Arrays;
public class practice_array {
	
	public static void main(String args[]) {
	int a[] = new int[5];
	a[0] = 1;
	a[1] = 12;
	a[2] = 22;
	a[3] = 78;
	a[4] = 67;
	
	 
	for(int i =0;i<a.length;i++) {
		System.out.println(i);
	}

	
	int b[][]= {{1,2,3}, {3,4,5}};
	System.out.println(b[0][1]);
}
}